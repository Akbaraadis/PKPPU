<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.project.pkppu">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PKPPU">
        <activity android:name=".user.AboutActivity"></activity>
        <activity android:name=".admin.skpk.DetailSkpkActivity" />
        <activity android:name=".admin.skmk.DetailSkmkActivity" />
        <activity android:name=".admin.skpk.SkpkAdminActivity" />
        <activity android:name=".admin.skmk.SkmkAdminActivity" />
        <activity android:name=".admin.HomeAdminActivity" />
        <activity android:name=".user.SkpkActivity" />
        <activity android:name=".user.SkmkActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".user.HomeUserActivity" />
    </application>

</manifest>